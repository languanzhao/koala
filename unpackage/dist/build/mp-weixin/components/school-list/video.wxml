<view><view class="schoolList-three"><view class="header"><view class="title">精彩图片</view></view><scroll-view class="scrollView" scroll-x="true"><block wx:for="{{videoList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['videoClick',['$0'],[[['videoList','',index,'school.id']]]]]]]}}" class="scrollView-item" catchtap="__e"><view class="video_image"><image src="{{item.image_url}}" mode="aspectFill"></image></view><view class="content"><view class="title">{{item.title}}</view><view class="bottom"><view class="imageBox"><image src="{{item.school.avatar}}" mode></image></view><view class="schoolName">{{''+item.school.name+''}}</view></view></view><view data-event-opts="{{[['tap',[['delBtn',['$0'],[[['videoList','',index,'id']]]]]]]}}" class="iconfont cancel" catchtap="__e"></view></view></block></scroll-view></view></view>